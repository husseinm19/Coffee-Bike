       <FrameLayout
           android:id="@+id/frame"
           android:padding="10dp"
           android:layout_width="match_parent"
           android:layout_height="match_parent">





       </FrameLayout>

   </com.shrikanthravi.customnavigationdrawer2.widget.SNavigationDrawer>



</RelativeLayout>